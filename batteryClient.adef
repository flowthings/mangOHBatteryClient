sandboxed: false
version: 1.0.0
start: manual


executables:
{
    batteryClient = ( batteryClientComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = INFO
    }
    run:
    {
        ( batteryClient )
    }
    faultAction: restart
}

bindings:
{
    batteryClient.batteryClientComponent.ma_battery -> batteryService.ma_battery
    batteryClient.batteryClientComponent.ma_adminbattery -> batteryService.ma_adminbattery
}
